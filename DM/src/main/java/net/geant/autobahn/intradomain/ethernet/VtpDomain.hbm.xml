<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"hibernate-mapping-3.0.dtd">
<hibernate-mapping package="net.geant.autobahn.intradomain.ethernet">
    <class name="VtpDomain" table="VTP_DOMAIN" schema="public">
        <id name="vtpDomainId" type="long">
            <column name="vtp_domain_id" />
            <generator class="increment" />
        </id>
        <property name="name" type="string">
            <column name="name" />
        </property>
        <property name="srvIpv4Addr" type="string">
            <column name="srv_ipv4_addr" />
        </property>
    </class>
</hibernate-mapping>
