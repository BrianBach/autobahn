<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"hibernate-mapping-3.0.dtd">
<hibernate-mapping package="net.geant2.jra3.intradomain.ethernet">
    <class name="VlanPort" table="vlan_port" schema="public">
        <composite-id>
            <key-many-to-one name="genericInterface" column="interface_id" class="net.geant2.jra3.intradomain.common.GenericInterface">
            </key-many-to-one>
        </composite-id>
        <many-to-one name="vlan" class="Vlan" fetch="select" cascade="save-update">
            <column name="vlan_id" /> 
        </many-to-one>
    </class>
</hibernate-mapping>
