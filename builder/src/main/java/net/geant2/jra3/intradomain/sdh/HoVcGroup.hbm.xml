<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"hibernate-mapping-3.0.dtd">
<hibernate-mapping package="net.geant2.jra3.intradomain.sdh">
    <class name="HoVcGroup" table="ho_vc_group" schema="public">
        <composite-id>
        	<key-many-to-one name="hoVcGroup" column="ho_vc_group_id" class="net.geant2.jra3.intradomain.common.GenericConnection">
            </key-many-to-one>
        </composite-id>
        <many-to-one name="vlanTag" class="VlanTag" fetch="select" cascade="save-update">
            <column name="vlan_tag_id" /> 
        </many-to-one>
        <property name="name" type="string">
            <column name="name" />
        </property>
    </class>
</hibernate-mapping>
