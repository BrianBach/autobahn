<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"hibernate-mapping-3.0.dtd">
<hibernate-mapping package="net.geant2.jra3.intradomain.sdh">
    <class name="OpsLink" table="ops_link" schema="public">
        <composite-id>
        	<key-many-to-one name="opsLink" column="ops_link_id" class="net.geant2.jra3.intradomain.common.GenericLink">
            </key-many-to-one>
        </composite-id>
        <property name="maxNoLambdas" type="long">
            <column name="max_no_lambdas" />
        </property>
        <property name="bitrate" type="long">
            <column name="bitrate" />
        </property>
        <property name="status" type="string">
            <column name="status" />
        </property>
    </class>
</hibernate-mapping>
