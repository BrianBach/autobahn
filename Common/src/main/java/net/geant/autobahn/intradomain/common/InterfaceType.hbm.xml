<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"hibernate-mapping-3.0.dtd">
<hibernate-mapping package="net.geant.autobahn.intradomain.common">
    <class name="InterfaceType" table="INTERFACE_TYPE" schema="public">
        <id name="interfaceTypeId" type="long">
            <column name="interface_type_id" />
            <generator class="increment" />
        </id>
        <property name="switchingType" type="string">
            <column name="switching_type" />
        </property>
        <property name="dataEncodingType" type="string">
            <column name="data_encoding_type" />
        </property>
    </class>
</hibernate-mapping>
