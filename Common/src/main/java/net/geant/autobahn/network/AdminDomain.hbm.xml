<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"hibernate-mapping-3.0.dtd">

<hibernate-mapping package="net.geant.autobahn.network">
	
	<class name="AdminDomain" table="ADMIN_DOMAIN" lazy="false">
		<id name="bodID" column="domain_id" type="string" />
		
		<property name="ASID" type="string" column="asid" />
		
		<property name="name" type="string" column="ad_name" />
		
		<property name="clientDomain" type="boolean" column="client_domain" />
		
		<property name="idcpServer" type="string" column="idcp_server" />
	</class>
	
</hibernate-mapping>